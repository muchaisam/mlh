<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginStart="23dp"
    android:layout_marginEnd="23.2dp"
    tools:context=".AccountActivity">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/sfbold"
        android:letterSpacing="0.0325"
        android:text="@string/ac"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/image"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="3dp"
        android:padding="5dp"
        android:src="@drawable/ac"
        android:background="@color/white"
        android:contentDescription="@string/p" />

    <ImageView
        android:layout_toEndOf="@id/image"
        android:layout_width="30dp"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="250dp"
        android:layout_marginTop="100dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_arrow_forward_black_24dp"
        android:contentDescription="@string/arrow" />

    <TextView
        android:id="@+id/text"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:gravity="center_vertical"
        android:layout_marginTop="105dp"
        android:text="@string/account"
        android:letterSpacing="0.0325"
        android:fontFamily="@font/sfbold"
        android:textSize="15sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/image1"
        android:layout_below="@+id/image"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="3dp"
        android:padding="3dp"
        android:background="@color/white"
        android:src="@drawable/sett"
        android:contentDescription="@string/p" />

    <ImageView
        android:layout_below="@+id/image"
        android:layout_toEndOf="@id/image1"
        android:layout_width="30dp"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="250dp"
        android:layout_marginTop="40dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_arrow_forward_black_24dp"
        android:contentDescription="@string/arrow" />

    <TextView
        android:id="@+id/text1"
        android:layout_below="@+id/text"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="45dp"
        android:text="@string/set"
        android:fontFamily="@font/sfbold"
        android:letterSpacing="0.0325"
        android:textSize="15sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/image2"
        android:layout_below="@+id/image1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="3dp"
        android:padding="3dp"
        android:background="@color/white"
        android:src="@drawable/transac"
        android:contentDescription="@string/tr" />

    <ImageView
        android:layout_below="@+id/image1"
        android:layout_toEndOf="@id/image1"
        android:layout_width="30dp"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="250dp"
        android:layout_marginTop="40dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_arrow_forward_black_24dp"
        android:contentDescription="@string/arrow" />

    <TextView
        android:id="@+id/text2"
        android:layout_below="@+id/text1"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="50dp"
        android:text="@string/tr"
        android:fontFamily="@font/sfbold"
        android:letterSpacing="0.0325"
        android:textSize="15sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/image3"
        android:layout_below="@+id/image2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="3dp"
        android:padding="3dp"
        android:background="@color/white"
        android:src="@drawable/logout"
        android:contentDescription="@string/p" />

    <ImageView
        android:layout_below="@+id/image2"
        android:layout_toEndOf="@id/image1"
        android:layout_width="30dp"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="250dp"
        android:layout_marginTop="40dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_arrow_forward_black_24dp"
        android:contentDescription="@string/arrow" />

    <TextView
        android:id="@+id/text3"
        android:layout_below="@+id/text2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginTop="50dp"
        android:text="@string/log"
        android:fontFamily="@font/sfbold"
        android:letterSpacing="0.0325"
        android:textSize="15sp"
        android:textStyle="bold" />

</RelativeLayout>